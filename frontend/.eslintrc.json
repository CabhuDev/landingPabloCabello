{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "gtag": "readonly",
    "hj": "readonly",
    "dataLayer": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "curly": "off",
    "no-undef": "error"
  }
}
